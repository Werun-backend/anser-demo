<anserTest>
    <testCases>
        <baseUrl>http://localhost:8080</baseUrl>
        <var>testNumber</var>
        <var>testPassword</var>
        <var>token</var>
        <var>name</var>
        <var>outputNumber</var>
        <testCase>
            <name>测试登录账号并获得个人信息 比对登录信息和获得的个人信息</name>
            <setVar>
                <var>testNumber</var>
                <value>2022212005</value>
            </setVar>
            <setVar>
                <var>testPassword</var>
                <value>2022212005</value>
            </setVar>
            <api>
                <url>/user/login</url>
                <method>POST</method>
                <request>
                    <body>
                        <key>number</key>
                        <value>testNumber</value>
                    </body>
                    <body>
                        <key>password</key>
                        <value>testPassword</value>
                    </body>
                </request>
                <response>
                    <body>
                        <var>token</var>
                        <value>data.token</value>
                    </body>
                    <body>
                        <var>outputNumber</var>
                        <value>data.userInformationVo.number</value>
                    </body>
                </response>
            </api>
            <api>
                <url>/user/information</url>
                <method>GET</method>
                <request>
                    <header>
                        <key>satoken</key>
                        <value>token</value>
                    </header>
                    <body>
                        <key>number</key>
                        <value>testNumber</value>
                    </body>
                    <body>
                        <key>password</key>
                        <value>testPassword</value>
                    </body>
                </request>
                <response>
                    <body>
                        <var>name</var>
                        <value>data.username</value>
                    </body>
                </response>
            </api>
            <assert>
                <var>outputNumber</var>
                <match>testNumber.value</match>
            </assert>
            <assert>
                <var>name</var>
                <match>张沣睿</match>
            </assert>
        </testCase>
        <testCase>
            <name>测试登录账号并获得个人信息 比对登录信息和获得的个人信息</name>
            <setVar>
                <var>testNumber</var>
                <value>2022212005</value>
            </setVar>
            <setVar>
                <var>testPassword</var>
                <value>2022212005</value>
            </setVar>
            <api>
                <url>/user/login</url>
                <method>POST</method>
                <request>
                    <body>
                        <key>number</key>
                        <value>${testNumber.value}</value>
                    </body>
                    <body>
                        <key>password</key>
                        <value>${testPassword.value}</value>
                    </body>
                </request>
                <response>
                    <body>
                        <var>token</var>
                        <value>#{data.token}</value>
                    </body>
                    <body>
                        <var>outputNumber</var>
                        <value>#{data.userInformationVo.number}</value>
                    </body>
                </response>
            </api>
            <api>
                <url>/user/information</url>
                <method>GET</method>
                <request>
                    <header>
                        <key>satoken</key>
                        <value>${token.value}</value>
                    </header>
                    <body>
                        <key>number</key>
                        <value>${testNumber.value}</value>
                    </body>
                    <body>
                        <key>password</key>
                        <value>${testPassword.value}</value>
                    </body>
                </request>
                <response>
                    <body>
                        <var>name</var>
                        <value>#{data.username}</value>
                    </body>
                </response>
            </api>
            <assert>
                <var>outputNumber</var>
                <match>${testNumber.value}</match>
            </assert>
            <assert>
                <var>name</var>
                <match>张睿</match>
            </assert>
        </testCase>
    </testCases>
</anserTest>